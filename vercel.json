{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server.js"
        },
        {
            "src": "/(.*\\.html)",
            "dest": "/$1"
        },
        {
            "src": "/(.*\\.css)",
            "dest": "/$1"
        },
        {
            "src": "/(.*\\.js)",
            "dest": "/$1"
        },
        {
            "src": "/(.*\\.png)",
            "dest": "/$1"
        },
        {
            "src": "/(.*\\.jpg)",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ]
}